<?xml version="1.0" encoding="utf-8"?>
<component version="1.2" xmlns="http://schemas.circuit-diagram.org/circuitDiagramDocument/2012/component/xml">
  <declaration>
    
    <!-- Describe the component -->
    <meta name="name" value="Check Valve" />
    <meta name="version" value="1.0" />
    <meta name="minsize" value="120" />
    <meta name="author" value="Corsaka" />
    <meta name="additionalinformation" value="Pnematic component" />
    <meta name="guid" value="de9d2a33-3475-43c2-b610-52a2cbe03c92" />
    
    <property name="Type" type="enum" default="Standard" serialize="am" display="Loaded">
      <option>Standard</option>
      <option>Spring</option>
    </property>
    
    <configurations>
      <configuration name="Check Valve" value="Type:Standard" />
      <configuration name="Check Valve (Spring Loaded)" value="Type:Spring" />
    </configurations>
    
  </declaration>
  
  <connections>
    
    <!-- Allow other components to connect to each of the pins -->
    <group>
      <connection start="_Start" end="_Middle-20x" edge="Start" />
      <connection start="_Middle+45x" end="_End" edge="End" />
    </group>
    
  </connections>
  
  <render>
    
    <!-- Standard -->
    <group>
      <line start="_Start" end="_Middle-20x" />
      <line start="_Middle-20x" end="_Middle+20y" />
      <line start="_Middle-20x" end="_Middle-20y" />
      <ellipse centre="_Middle+4x" rx="15.5" ry="15.5" />
      <line start="_Middle+20x" end="_End" />
    </group>
    <!-- Loaded -->
    <group conditions="$Type==Spring">
      <line start="_Middle+20x" end="_Middle+22.5x+10y" />
      <line start="_Middle+22.5x+10y" end="_Middle+27.5x-10y" />
      <line start="_Middle+27.5x-10y" end="_Middle+32.5x+10y" />
      <line start="_Middle+32.5x+10y" end="_Middle+37.5x-10y" />
      <line start="_Middle+37.5x-10y" end="_Middle+42.5x+10y" />
    </group>
    
  </render>
</component>
