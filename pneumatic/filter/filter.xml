<?xml version="1.0" encoding="utf-8"?>
<component version="1.2" xmlns="http://schemas.circuit-diagram.org/circuitDiagramDocument/2012/component/xml">
  <declaration>
    
    <!-- Describe the component -->
    <meta name="name" value="Filter" />
    <meta name="version" value="1.0" />
    <meta name="minsize" value="120" />
    <meta name="author" value="Corsaka" />
    <meta name="additionalinformation" value="Pnematic component" />
    <meta name="guid" value="de24a8e2-5afc-43ce-ac79-e24fe2aebfae" />
    
    <!-- Can only be displayed horizontally -->
    <flags>
      <option>horizontalonly</option>
    </flags>
    
  </declaration>
  <connections>
    
    <!-- Allow other components to connect to each of the pins -->
    <group>
      <connection start="_Start-20x" end="_Middle-40x" edge="Start" />
      <connection start="_Middle+40x" end="_End+20x" edge="End" />
    </group>
    
  </connections>
  <render>
    
    <group>
      <!-- Draw the component -->
      <line start="_Start-20x" end="_Start+20x" />
      <line start="_Middle-40x" end="_Middle+40y" />
      <line start="_Middle-40x" end="_Middle-40y" />
      <line start="_Middle+40y" end="_Middle+40x" />
      <line start="_Middle-40y" end="_Middle+40x" />
      <line start="_Middle-40y" end="_Middle-25y" />
      <line start="_Middle-20y" end="_Middle-5y" />
      <line start="_Middle" end="_Middle+15y" />
      <line start="_Middle+20y" end="_Middle+40y" />
      <line start="_End-20x" end="_End+20x" />
    </group>
    
  </render>
</component>
